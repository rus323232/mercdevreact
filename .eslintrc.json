{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "no-undef": 0,
    "semi": "error",
    "no-param-reassign": 0,
    "quotes": ["error", "single"],
    "import/prefer-default-export": 0,
    "react/jsx-props-no-spreading": 0,
    "react/destructuring-assignment": 0,
    "arrow-parens": [2, "as-needed", { "requireForBlockBody": true }]
  }
}
